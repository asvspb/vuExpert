{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": ["http://localhost:4173"],
      "startServerCommand": "npm run preview",
      "startServerReadyPattern": "Local:",
      "numberOfRuns": 1,
      "settings": {
        "onlyCategories": ["accessibility"]
      }
    },
    "assert": {
      "assertions": {
        "categories:accessibility": [
          "error",
          {"minScore": 0.9}
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
 }
}
